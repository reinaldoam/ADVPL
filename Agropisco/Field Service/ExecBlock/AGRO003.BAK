//MONTA O CÓDGIO DO PRODUTO
User Function AGRO003()
//Salva a área do SX7
Local lRet := .T. 

cAlias := Alias()
dbSelectArea("SB1")
dbSetOrder(1)
dbSeek(xFilial("SB1")+aCols[N][2]+"01")

If M->AB5_VUNIT < SB1->B1_PRV1
	lRet := .F.
	Alert("Não é permitido um valor menor do que preço de venda!")
Else
	lRet := .T.
EndIf

dbSelectArea(cAlias)

Return(lRet)
