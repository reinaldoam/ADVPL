dbSelectArea("SX3")
dbSetOrder(1)
DbSeek("AB3")
While ( !Eof() .And. SX3->X3_ARQUIVO == "AB3" )
	uCampo := SX3->X3_CAMPO
	If ( SX3->X3_CONTEXT == "V" )
		M->&(uCampo) := CriaVar(uCampo,.T.)
