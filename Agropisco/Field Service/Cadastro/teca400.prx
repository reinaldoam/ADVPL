If ( Pergunte("ATA400",.T.) )
	If Recmoeda( dDatabase, MV_PAR07 ) <= 0
		//"Não existe taxa cadastrada para a moeda selecionada: " # "Atenção"
		MsgAlert(STR0034 + StrZero(MV_PAR07,1), STR0035)
