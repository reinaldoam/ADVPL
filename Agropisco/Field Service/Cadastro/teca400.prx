	Next nCntFor
	aAux := {}
	dbSelectArea("AB5")
	dbSetOrder(1)
	If ( DbSeek(xFilial("AB5")+AB3->AB3_NUMORC+AB4->AB4_ITEM) )
      While ( !Eof() .And. xFilial("AB5") == AB5->AB5_FILIAL .And.;
